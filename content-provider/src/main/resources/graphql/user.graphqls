
schema {
	query: Query
	mutation: Mutation
}

type User {
	id: ID!
	name: String!
	email: String!
	isAdmin: Boolean
	password: String!
}
# Root
type Query {
	findAllUsers: [User]!
	countUsers: Int
}
# Root
type Mutation {
	createUser(input: UserInput): User!
}

input UserInput {
	name: String!
	email: String!
	isAdmin: Boolean
	password: String!
}